<ivy-module version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd" xmlns:m="http://ant.apache.org/ivy/maven">
  <info organisation="org.opendof.tools-interface-repository" module="interface-repository-servlet" revision="2.0">
    <license name="ISC License" url="https://opendof.org/license-code/"/>
    <repository name="org.opendof" url="https://asset.opendof.org/" pattern="ivy2/[organisation]/[module]/[revision]/ivy(.[platform]).xml" ivys="true" artifacts="true"/>
    <description homepage="https://opendof.org/">
The Interface Repository Servlet Library implements a Java servlet, providing web access to the Interface Repository.
    </description>
  </info>
  <configurations>
    <conf name="default" visibility="public" description="runtime dependencies and master artifact can be used with this conf" extends="runtime,master"/>
    <conf name="master" visibility="public" description="contains only the artifact published by this module itself, with no transitive dependencies"/>
    <conf name="compile" visibility="public" description="this is the default scope, used if none is specified. Compile dependencies are available in all classpaths."/>
    <conf name="runtime" visibility="public" description="this scope indicates that the dependency is not required for compilation, but is for execution. It is in the runtime and test classpaths, but not the compile classpath." extends="compile"/>
    <conf name="test" visibility="private" description="this scope indicates that the dependency is not required for normal use of the application, and is only available for the test compilation and execution phases." extends="runtime"/>
    <conf name="sources" visibility="public" description="this configuration contains the source artifact of this module, if any."/>
    <conf name="javadoc" visibility="public" description="this configuration contains the javadoc artifact of this module, if any."/>
    <conf name="optional" visibility="public" description="contains all optional dependencies"/>
    <conf name="dist" visibility="public" description="contains only the standard, binary distribution, with no transitive dependencies"/>
  </configurations>
  <publications>
    <artifact conf="sources" name="${ivy.module.group}-${ivy.module}" type="source" ext="src.tar.gz"/>
    <artifact conf="sources" name="${jar.name}" type="source" ext="jar" m:classifier="sources"/>
    <artifact conf="dist" name="${ivy.module.group}-${ivy.module}" type="dist" ext="noarch.tar.gz"/>
    <artifact conf="master" name="${jar.name}" type="jar" ext="jar"/>
    <artifact conf="javadoc" name="${jar.name}" type="javadoc" ext="jar" m:classifier="javadoc"/>
  </publications>
  <dependencies>
    <dependency org="org.opendof.tools-interface-repository" name="interface-repository-core" rev="[2,3)" conf="compile->master,compile;javadoc"/>
    <dependency org="org.opendof.tools-interface-repository" name="interface-repository-opendof" rev="[2,3)" conf="compile->master,compile;javadoc"/>
    <dependency org="javax.servlet" name="javax.servlet-api" rev="[3.1,4)" conf="compile->master,compile"/>
	<dependency org="ch.qos.logback" name="logback-classic" rev="[1.1.3,2)" conf="compile->master,compile"/>
	<dependency org="ch.qos.logback" name="logback-core" rev="[1.1.3,2)" conf="compile->master,compile"/>
    <dependency org="com.github.scribejava" name="scribejava-core" rev="2.4.0"/>
    <dependency org="org.apache.wicket" name="wicket-core" rev="7.2.0"/>
    <dependency org="org.apache.wicket" name="wicket-auth-roles" rev="7.2.0"/>
	<dependency org="org.apache.wicket" name="wicket-extensions" rev="7.2.0"/>
	<dependency org="org.opendof.tools-interface-repository" name="ext-oauth" rev="20100527" conf="compile->master,compile"/>
	<dependency org="org.opendof.tools-interface-repository" name="ext-oauth-consumer" rev="20100527" conf="compile->master,compile"/>
	<dependency org="org.opendof.tools-interface-repository" name="ext-oauth-httpclient4" rev="20090913" conf="compile->master,compile"/>
  </dependencies>
</ivy-module>
